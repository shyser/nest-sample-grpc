{
  "name": "server",
  "version": "1.0.0",
  "description": "nest-sample-grpc-server",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "dependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/microservices": "^5.0.0",
    "@nestjs/testing": "^5.0.0",
    "@nestjs/websockets": "^5.0.0",
    "grpc": "^1.12.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typescript": "^2.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/node": "^9.3.0",
    "kill-port": "^1.2.0",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.1.0",
    "ts-node": "^6.0.0",
    "tsconfig-paths": "^3.3.1",
    "tslint": "5.3.2"
  },
  "homepage": "https://github.com/StephenSeraph/nest-sample-grpc"
